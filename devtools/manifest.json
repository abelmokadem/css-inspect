{
  "name": "CSS Inspect",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Toggle @support rules",
  "devtools_page": "devtools.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
